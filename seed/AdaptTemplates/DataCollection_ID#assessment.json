{
  "type": "DataCollection",
  "id": "ID#assessment",
  "dataViewDescription": "This data view captures student participation and performance data in state assessments for specific grade levels.",
  "fields": [
    {
      "type": "select",
      "id": "reportingYear",
      "label": "For what fiscal year?",
      "options": [
        {
          "label": "2024-2025",
          "value": "2025"
        },
        {
          "label": "2023-2024",
          "value": "2024"
        }
      ],
      "required": true,
      "shortLabel": "School year"
    },
    {
      "type": "select",
      "id": "reportingLevel",
      "label": "For what reporting level?",
      "options": [
        {
          "label": "SEA (State Education Agency)",
          "value": "SEA (State Education Agency)"
        },
        {
          "label": "LEA (Local Education Agency)",
          "value": "LEA (Local Education Agency)"
        },
        {
          "label": "SCH (School)",
          "value": "SCH (School)"
        }
      ],
      "required": true,
      "shortLabel": "Reporting level"
    }
  ],
  "files": [
