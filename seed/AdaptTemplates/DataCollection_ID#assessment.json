        "CSI": ["militaryconnectedstudentindicator"],
        "CSJ": ["ideadisabilitytype", "race"],
        "ST1": ["gradelevel", "proficiencystatus", "assessmenttypeadministered"]
      },
      "columns": [
        "file_record_number",
