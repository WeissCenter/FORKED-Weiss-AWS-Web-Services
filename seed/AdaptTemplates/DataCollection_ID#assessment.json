      "acceptedFileFormats": {
        "fieldAccept": ".csv",
        "fileInputHint": "Only CSV files are accepted."
      },
      "category_conditions": {
        "CSA": ["race"],
        "CSB": ["sex"],
        "CSC": ["ideadisabilitytype"],
        "CSD": ["englishlearnerstatus"],
