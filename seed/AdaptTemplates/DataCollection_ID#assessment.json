        "filler",
        "proficiencystatus",
        "programparticipationfostercare",
        "militaryconnectedstudentindicator",
        "TotalIndicator",
        "explanation",
        "assessmentcount"
      ],
      "conditions": [
        {
          "field": "scopeLevel",
          "operation": "neq",
          "value": "SCH"
        }
      ],
      "database": {
        "query": "select * from RDS.ReportEDFactsK12StudentAssessments where ReportCode = 'c175' and ReportLevel = 'sea' and ReportYear = ${reportingYear}"
      },
      "description": "The unduplicated number of students in grades 3 through 8 and in high school who completed the state assessment(s) in mathematics and received a valid score.",
      "included_in_all": ["gradelevel", "proficiencystatus", "assessmenttypeadministered"],
      "name": "FS175 - Academic Achievement in Mathematics",
      "report_code": "c175",
      "report_levels": {
        "lea": "LEA STUDENT PERFORMANCE MATH",
